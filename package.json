{
  "name": "votes-redux-full-stack",
  "version": "0.0.1",
  "description": "A live voting app built with full-stack Redux",
  "main": "index.js",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-preset-es2015": "^6.18.0",
    "immutable": "^3.8.1",
    "redux": "^3.6.0",
    "socket.io": "^1.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "jsdom": "^9.8.3",
    "mocha": "^3.2.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "scripts": {
    "start": "npm run start:backend",
    "start:backend": "babel-node index.js",
    "start:frontend": "webpack-dev-server",
    "test": "npm run mocha:compile -- \"test/**/*@(.js|.jsx)\"",
    "mocha:compile": "mocha --compilers js:babel-core/register --require ./test/test-helper.js",
    "test:backend": "npm run mocha:compile -- test/backend --recursive",
    "test:frontend": "npm run mocha:compile -- \"test/frontend/**/*@(.js|.jsx)\"",
    "test:watch": "npm run test -- --watch",
    "build": "webpack"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipochinskiy/full-stack-redux-tutorial.git"
  },
  "keywords": [],
